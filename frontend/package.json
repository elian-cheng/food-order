    "@mui/material": "^5.13.2",
    "@mui/system": "^5.13.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "mui-image": "^1.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.2",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@types/mui-image": "^1.0.1",
    "@types/node": "^20.2.1",
